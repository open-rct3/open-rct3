<{{ if onGamePage }}div{{ else }}a{{ /if }} {{ if !onGamePage }}href="/"{{ /if }} class="logo d-flex align-items-center align-self-end" aria-label="{{ siteTitle }}">
  <img src="/logo.png" alt="">
  {{ if onGamePage }}
  <button class="nub" title="Toggle {{ toggleTitle || "Notifications" }}"><i class="icon fas fa-bars"></i></button>
  {{ /if }}
  <span class="wordmark">{{ siteTitle }}</span>
</{{ if onGamePage }}div{{ else }}a{{ /if }}>
